<h1>Projects#index</h1>
<p>Find me in app/views/projects/index.html.erb</p>
<table border="2">
<tr>
 <th>name </th>
 <th>duration</th>
 <th> teamsize</th>
 <th>cost</th>
 <th> domain</th>
 
 </tr>
 <% @project.each do |b|%>
 <tr>
 
<td><%= b.name %></td>
<td><%= b.duration %></td>
<td><%= b.teamsize %></td>
<td><%= b.cost %></td>
<td><%= b.domain%></td>



<td><%= link_to 'show profile', user_show_path(b) %></td>
<td><%= link_to 'destroy', user_destroy_path(b), method: :delete,
data: {
confirm: 'Are you sure?'
	} %></td>

</tr>
<% end%>
</table>